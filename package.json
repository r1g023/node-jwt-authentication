{
  "name": "jwt-authentication",
  "version": "1.0.0",
  "description": "Authentication implementing JWT",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest --watch --verbose --runInBand --silent",
    "server": "cross-env NODE_ENV=development nodemon --require dotenv/config index.js",
    "start": "node index.js",
    "migrate": "knex migrate:latest",
    "seed": "knex seed:run",
    "rollback": "knex migrate:rollback",
    "migrateh": "heroku run knex migrate:latest -a YOUR_HEROKU_APP_NAME",
    "rollbackh": "heroku run knex migrate:rollback -a YOUR_HEROKU_APP_NAME",
    "seedh": "heroku run knex seed:run -a YOUR_HEROKU_APP_NAME",
    "databaseh": "heroku pg:psql -a YOUR_HEROKU_APP_NAME",
    "deploy": "git push heroku main"
  },
  "engines": {
    "node": "16.9.0",
    "npm": "7.21.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rigo0523/node-jwt-authentication.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rigo0523/node-jwt-authentication/issues"
  },
  "homepage": "https://github.com/rigo0523/node-jwt-authentication#readme",
  "dependencies": {
    "axios": "^0.21.4",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "faker": "^5.5.3",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.95.11",
    "knex-cleaner": "^1.3.1",
    "morgan": "^1.10.0",
    "pg": "^8.7.1",
    "sqlite3": "^5.0.2",
    "welcome": "^0.0.1"
  },
  "devDependencies": {
    "eslint": "^7.32.0",
    "nodemon": "^2.0.12"
  }
}
